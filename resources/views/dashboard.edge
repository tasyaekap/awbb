<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="../webix.css" type="text/css">
    <script src="../webix.js" type="text/javascript"></script>
    <link rel="stylesheet" href="https://cdn.materialdesignicons.com/2.7.94/css/materialdesignicons.css" type="text/css" charset="utf-8">

    <title>Document</title>
</head>
<body>
    <div id="cs"></div>
    <script>
        
data = "http://localhost:3333/showawb"
 var tabel = {
    view:"datatable",
    url:data,
    columns:[
             { id: "trnNoHAWB" , header : "No. AWB", width: 120 },
             { id: "Status" , header : "Status", width: 120 },
      		 { id: "TrnName" , header : "Ship. Name", width: 100 },
      		 { id: "trnAlm1" , header : "Alm1", width: 100 },
      		 { id: "trnAlm2" , header : "Alm2", width: 100 },
      		 { id: "trnAlm3" , header : "Alm3", width: 100 },
      		 { id: "trnCity" , header : "Kota", width: 100 },
      		 { id: "trnContact" , header : "Kontak", width: 100 },
      		 { id: "trnConsName" , header : "Cons. Name", width: 100 },
      		 { id: "trnConsAlm1" , header : "Alm1", width: 100 },
      		 { id: "trnConsAlm2" , header : "Alm2", width: 100 },
      		 { id: "trnConsAlm3" , header : "Alm3", width: 100 },
      		 { id: "trnConsCity" , header : "Kota", width: 100 },
      		 { id: "trnConsContact" , header : "Kontak", width: 100 },    		 
             { id: "trnchargeswt" , header : "Berat", width: 100 },
             { id: "trnKoli" , header : "Koli", width: 100 },
			 { id: "trnDim_H" , header : "Height", width: 100 },
      		 { id: "trnDim_W" , header : "Width", width: 100 },
      		 { id: "trnDim_L" , header : "Length", width: 100 },
             { id: "trnpickup" , header : "Pickup Data", width: 100 },
      		 { id: "trnpickuptime" , header : "Pickup Time", width: 100 },	 
      		 { id: "trnOrg" , header : "TLC ORG", width: 100 },
      		 { id: "trnDest" , header : "TLC DEST", width: 100 }
      	
      ] , 
    id:"tbl1", 
    select:"row",
    save:"rest->http://localhost:3333/awb"
      }
var menu_data = [
	{id: "dashboard", icon: "mdi mdi-view-dashboard", value: "Dashboards",  data:[
		{ id: "dashboard1", value: "Dashboard 1"},
		{ id: "dashboard2", value: "Dashboard 2"}
	]},
	{id: "layouts", icon: "mdi mdi-view-column", value:"Layouts", data:[
		{ id: "accordions", value: "Accordions"},
		{ id: "portlets", value: "Portlets"}
	]},
	{id: "tables", icon: "mdi mdi-table", value:"Data Tables", data:[
		{ id: "tables1", value: "Datatable"},
		{ id: "tables2", value: "TreeTable"},
		{ id: "tables3", value: "Pivot"}
	]},
	{id: "uis", icon: "mdi mdi-puzzle", value:"UI Components", data:[
		{ id: "dataview", value: "DataView"},
		{ id: "list", value: "List"},
		{ id: "menu", value: "Menu"},
		{ id: "tree", value: "Tree"}
	]},
	{id: "demo", icon: "mdi mdi-book", value:"Documentation"}
];

webix.ready(function(){
  webix.ui({ 
    rows: [
      { view: "toolbar", padding:3, elements: [
        { view: "icon", icon: "mdi mdi-menu", click: function(){
           $$("sidebar1").toggle();
         }
        },
        { view: "label", label: "My App"},
        {},
        { view: "icon", icon: "mdi mdi-comment",  badge:4},
        { view: "icon", icon: "mdi mdi-bell",  badge:10}
      ]
      },
      { cols:[
        { id:"sidebar1",
          view: "sidebar",
          data: menu_data,
          on:{
            onAfterSelect: function(id){
              webix.message("Selected: "+this.getItem(id).value)
            }
          }
        },
        {  type:"space",
  rows:[ 
    { 
					view:"toolbar",
					elements:[
						{ width:4 },
						{ view:"label", label:"Transactions", width:150 },
						{ minWidth:4 },
                        { view:"button", id:"sub", label:"Submitted", click: subb, width:"100" },
                        { view:"button", id:"nopod",label:"No POD", click: nopod, width:"80"},
                        { view:"button", id:"nosub",label:"No Submitted No POD", click: nosubb, width:"200"},
                        { view:"button", id:"cancel",label:"Cancel", click: cancel, width:"80"},
					]
				},
     tabel
  ]
}
      ]}
    ]
  });

  function cancel (){ 
  //get user input value
  var value = "Cancelled"
  $$("tbl1").filter(function(obj){
    return obj.Status.toLowerCase().indexOf(value)!=-1;
  })
};

  function nopod (){ 
  //get user input value
  var value = "No POD"
  $$("tbl1").filter(function(obj){
    return obj.Status.toLowerCase().indexOf(value)!=-1;
  })
};

  function nosubb (){ 
  //get user input value
  var value = "Not Submitted"
  $$("tbl1").filter(function(obj){
    return obj.Status.toLowerCase().indexOf(value)!=-1;
  })
};

function subb (){ 
  //get user input value
  var value = "Submitted"
  webix.message("All is correct");
  $$("tbl1").filter(function(obj){
    return obj.Status.toLowerCase().indexOf(value)!=-1;
  })
};
  

}); 
    </script>
    
</body>
</html>